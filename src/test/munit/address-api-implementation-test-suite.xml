<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:munit="http://www.mulesoft.org/schema/mule/munit" xmlns:munit-tools="http://www.mulesoft.org/schema/mule/munit-tools"
	xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xsi:schemaLocation="
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/munit http://www.mulesoft.org/schema/mule/munit/current/mule-munit.xsd
		http://www.mulesoft.org/schema/mule/munit-tools  http://www.mulesoft.org/schema/mule/munit-tools/current/mule-munit-tools.xsd">
	<munit:config doc:name="MUnit configuration" doc:id="3405ffd5-2f18-4f94-a826-e743b717848a" name="firstParameterization">
		<munit:parameterizations >
			<munit:parameterization name="inputfileParameterized" >
				<munit:parameters >
					<munit:parameter propertyName="inputfile" value="address.json" />
				</munit:parameters>
			</munit:parameterization>
		</munit:parameterizations>
	</munit:config>
	<munit:test name="address-api-implementation-test-suite-insertAddressFlowTest" doc:id="353e4373-75cf-4dae-83fb-21d5dc89686c" description="Test">
		<munit:behavior >
			<munit-tools:spy doc:name="Spy" doc:id="114cd8f8-cd36-49a4-8967-0551ec6157d9" processor="ee:transform">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="Form Address Table Data" attributeName="doc:name" />
				</munit-tools:with-attributes>
				<munit-tools:before-call>
					<munit-tools:assert-that doc:name="Assert that" doc:id="5e27f598-608e-486f-ab58-499a0bbcfb25" expression="#[vars.addressDates.createDate]" is="#[MunitTools::notNullValue()]"/>
				</munit-tools:before-call>
				<munit-tools:after-call >
					<munit-tools:assert-that doc:name="Assert that" doc:id="c1ced5d9-b515-4c30-8abb-9e371c8a5fee" is="#[MunitTools::notNullValue()]" expression="#[vars.addressTableData.CREATEDATE]"/>
				</munit-tools:after-call>
			</munit-tools:spy>
		</munit:behavior>
		<munit:execution >
			<munit:set-event doc:name="Set Event" doc:id="e54a5670-df67-4eff-9354-e9655091fed0" >
				<munit:payload value='#[MunitTools::getResourceAsString("${inputfile}")]' mediaType="application/json" />
			</munit:set-event>
			<flow-ref doc:name="Flow-ref to insertAddressFlow" doc:id="6060663c-5b0d-441b-b167-89f0488b608c" name="insertAddressFlow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-that doc:name="Assert that" doc:id="f82602dd-877e-4e66-a92e-e1772637f082" expression="#[payload]" is='#[MunitTools::equalTo("Insert Successful")]'/>
		</munit:validation>
	</munit:test>


</mule>
